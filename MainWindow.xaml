<Window x:Class="CookMaster.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CookMaster"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button Content="Register" Command="{Binding OpenRegisterWindowCommand}" Width="150" Height="75"/>
        <Button Content="Recipe List" Command="{Binding OpenRecipeListWindowCommand}" Width="150" Height="75"/>
    </StackPanel>
</Window>
